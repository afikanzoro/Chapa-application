<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chmarax.transformers">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_noshadow"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_noshadow"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.chmarax.transformers.MainActivity" />
        <activity android:name="com.example.chmarax.transformers.RegistrationActivity" />
        <activity android:name="com.example.chmarax.transformers.SplashScrActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.chmarax.transformers.LoginActivity" />
        <activity android:name="com.example.chmarax.transformers.PWresetActivity"></activity>
    </application>

</manifest>